{
    "bootstrap": "vendor/autoload.php",
    "path": "benchmarks",
    "php_config": {
        "memory_limit": "1G"
    },
    "extensions": [
        "PhpBench\\Extensions\\XDebug\\XDebugExtension",
        "PhpBench\\Extensions\\Dbal\\DbalExtension"
    ],
    "reports": {
        "all": {
            "generator": "composite",
            "reports": ["env", "default", "aggregate" ]
        }
    }
}
