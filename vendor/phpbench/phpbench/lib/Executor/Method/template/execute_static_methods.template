<?php

$class = '{{ class }}';
$file = '{{ file }}';
$methods = {{ methods }};
$bootstrap = '{{ bootstrap }}';

if ($bootstrap) {
    call_user_func(function () use ($bootstrap) {
        require_once($bootstrap);
    });
}

require_once($file);

foreach ($methods as $method) {
    $class::$method();
}

echo serialize(array());

exit(0);
